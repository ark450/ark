        function validate() {
            var cd1 = document.getElementById("codee1");
            var cd2 = document.getElementById("codees2");
            if(cd1.value != "" && cd2.value != ""){
                if(cd1.value == cd2.value){
                    return true;
                }
            }
            alert("The values should be equal and not blank");
            return false;


